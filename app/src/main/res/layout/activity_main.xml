<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/txtcocheMain"
        android:layout_width="344dp"
        android:layout_height="45dp"
        android:layout_gravity="center"
        android:layout_marginTop="80sp"
        android:layout_marginEnd="0sp"
        android:layout_marginBottom="0sp"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="34sp" />

    <Button
        android:id="@+id/btnCocheMain"
        android:layout_width="346dp"
        android:layout_height="47dp"
        android:layout_gravity="center"
        android:layout_marginTop="20sp"
        android:gravity="center"
        android:text="Añadir Coche"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/txtMotoMain"
        android:layout_width="344dp"
        android:layout_height="45dp"
        android:layout_gravity="center"
        android:layout_marginTop="100sp"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="34sp" />

    <Button
        android:id="@+id/btnMotoMain"
        android:layout_width="346dp"
        android:layout_height="47dp"
        android:layout_gravity="center"
        android:layout_marginTop="20sp"
        android:gravity="center"
        android:text="Añadir Moto"
        tools:ignore="TouchTargetSizeCheck" />

    <TextView
        android:id="@+id/txtBiciMain"
        android:layout_width="344dp"
        android:layout_height="45dp"
        android:layout_gravity="center"
        android:layout_marginTop="100sp"
        android:gravity="center"
        android:text="TextView"
        android:textAlignment="center"
        android:textSize="34sp" />

    <Button
        android:id="@+id/btnBiciMain"
        android:layout_width="346dp"
        android:layout_height="47dp"
        android:layout_gravity="center"
        android:layout_marginTop="20sp"
        android:gravity="center"
        android:text="Añadir Bici"
        tools:ignore="TouchTargetSizeCheck" />
</LinearLayout>